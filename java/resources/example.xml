<?xml version="1.0" encoding="UTF-8"?>
<questionnaire>
  <initial>q0 q3 q6</initial>
  <questions>
    <question id="q0" variable="vo">
      <condition>true</condition>
      <label>Wat is je vooropleiding?</label>
      <next>q1 q4 q5</next>
    </question>
    <question id="q1" variable="sc">
      <condition>(vo === "HBO")</condition>
      <label>Moet je schakelen?</label>
      <next>q2</next>
    </question>
    <question id="q2" variable="cijfer">
      <condition>(sc === "ja")</condition>
      <label>Wat is je cijfer?</label>
      <next />
    </question>
    <question id="q3" variable="lt">
      <condition>true</condition>
      <label>Hoe oud ben je?</label>
      <next>q4</next>
    </question>
    <question id="q4" variable="wz">
      <condition>((vo === "WO")) &amp;&amp; ((lt === "17"))</condition>
      <label>Weet je het zeker?</label>
      <next />
    </question>
    <question id="q5" variable="uni">
      <condition>(vo === "WO")</condition>
      <label>Wat is de universiteit?</label>
      <next />
    </question>
    <question id="q6" variable="hcom">
      <condition>true</condition>
      <label>Commentaar?</label>
      <next>q6 q7</next>
    </question>
    <question id="q7" variable="com">
      <condition>(hcom === "ja")</condition>
      <label>Je commentaar:</label>
      <next />
    </question>
  </questions>
</questionnaire>

